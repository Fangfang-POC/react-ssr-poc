    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@babel/core": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^7.2.2",
    "html-webpack-plugin": "^5.3.2",
    "nodemon": "^2.0.18",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.3.0",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-node-externals": "^3.0.0",
    "webpack-merge": "^5.8.0"
